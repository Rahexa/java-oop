package oop;

public class car {
	String Brand,Type,Model;
	int Chessisno;
	
	
	
	void setinformation(String B,String T,String M,int C) {
		
		Brand = B;
		Type  = T;
		Model = M;
		
		Chessisno = C;
		
		
	}
	
	
	void displayinformation() {
		System.out.println(Brand);
		System.out.println(Type);

		System.out.println(Model);

		System.out.println(Chessisno);

		
		
		
	}

}
